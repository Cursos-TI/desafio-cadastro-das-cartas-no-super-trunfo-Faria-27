#include <stdio.h>

int main () {

    float = n1, n2, n3;
    float = media;
    media = (n1+n2+n3)/3;

    printf("Digite a 1º nota: \n");
    scanf("%f", &n1);

    printf("Digite a 2º nota: \n");
    scanf("%f", &n2);

    printf("Digite a 3º nota: \n");
    scanf("%f", &n3);

    printf("A média das notas é : %f", media);

    return 0;
}
